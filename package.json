{
    "name": "stock-prices",
    "version": "1.0.0",
    "description": "Stock Prices API built with Express.js and TypeScript",
    "main": "dist/index.js",
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8.0.0"
    },
    "scripts": {
        "dev": "nodemon",
        "build": "tsc",
        "start": "node dist/index.js",
        "clean": "rm -rf dist",
        "rebuild": "pnpm clean && pnpm build",
        "format": "prettier --write \"src/**/*.{ts,js,json}\"",
        "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
        "heroku-postbuild": "pnpm build"
    },
    "keywords": [
        "express",
        "typescript",
        "api",
        "stock-prices"
    ],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.24.0",
    "dependencies": {
        "@types/express": "^5.0.6",
        "@types/node": "^25.1.0",
        "express": "^5.2.1",
        "typescript": "^5.9.3"
    },
    "devDependencies": {
        "nodemon": "^3.1.11",
        "prettier": "^3.8.1",
        "ts-node": "^10.9.2",
        "tsx": "^4.21.0"
    }
}
